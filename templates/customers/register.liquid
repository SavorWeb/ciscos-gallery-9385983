<ol class="breadcrumb">
  <li><a href="/">Home</a></li>
  <li>Create account</li>
</ol>

<div class="collection-title">
  <h2 class="p0 m0 center">
    {{ settings.registration-headline }}
  </h2>
  <p class="center">{{ settings.registration-text }}</p>
</div>

<div class="customer-template mid">
  <div id="customer">
    <!-- Create Customer -->
    <div id="create-customer">

      {% form 'create_customer' %}
        {{ form.errors | default_errors }}

        <div id="first_name" class="form-group">
          <label for="first_name" class="login">First Name</label>
          <input type="text" value="" name="customer[first_name]" id="first_name" class="large" size="30" />
        </div>

        <div id="last_name" class="form-group">
          <label for="last_name" class="login">Last Name</label>
          <input type="text" value="" name="customer[last_name]" id="last_name" class="large" size="30" />
        </div>


        <div id="interests" class="interests clearfix">
            
            <label for="interests">Interests</label>
            
            <div id="AmericanIndian">
              <input type="checkbox" value="Interested" name="customer[note][American Indian]" id="AmericanIndian" />
              <label for="AmericanIndian" class="login">American Indian</label>
            </div>

            <div id="CowboyWestern">
              <input type="checkbox" value="Interested" name="customer[note][Cowboy Western]" id="CowboyWestern" />
              <label for="CowboyWestern" class="login">Cowboy Western</label>
            </div>

            <div id="FineArt">
              <input type="checkbox" value="Interested" name="customer[note][Cowboy Western]" id="FineArt" />
              <label for="FineArt" class="login">Fine Art</label>
            </div>

            <div id="Jewelry">
              <input type="checkbox" value="Interested" name="customer[note][Jewelry]" id="Jewelry" />
              <label for="Jewelry" class="login">Jewelry</label>
            </div>

            <div id="SportingGoods">
              <input type="checkbox" value="Interested" name="customer[note][Sporting Goods]" id="SportingGoods" />
              <label for="SportingGoods" class="login">Sporting Goods</label>
            </div>

            <div id="LodgeFurnishings">
              <input type="checkbox" value="Interested" name="customer[note][Lodge Furnishings]" id="LodgeFurnishings" />
              <label for="LodgeFurnishings" class="login">Lodge Furnishings</label>
            </div>

            <div id="SpecialCollections">
              <input type="checkbox" value="Interested" name="customer[note][Special Collections]" id="SpecialCollections" />
              <label for="SpecialCollections" class="login">Special Collections</label>
            </div>

        </div>

        <div id="referral" class="referral clearfix">
            <label for="referral">How did you hear about us?</label>
            
            <div>
              <input type="radio" value="Friend" name="customer[note][Referral]" id="referral" /><span>Friend</span>
            </div>
            
            <div>
              <input type="radio" value="Show" name="customer[note][Referral]" id="referral" /><span>Show</span>
            </div>
            
            <div>
              <input type="radio" value="Magazine" name="customer[note][Referral]" id="referral" /><span>Magazine</span>
            </div>
            
            <div>
              <input type="radio" value="Internet Search" name="customer[note][Referral]" id="referral" /><span>Internet Search
            </span></div>
            
            <div>
              <input type="radio" value="Newspaper" name="customer[note][Referral]" id="referral" /><span>Newspaper</span>
            </div>
            
            <div>
              <input type="radio" value="Store Visit" name="customer[note][Referral]" id="referral" /><span>Store Visit</span>
            </div>
        </div>
<div class="clearfix"></div>
