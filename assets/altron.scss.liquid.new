@mixin grayscale 
{ 
  filter: grayscale(100%);
  filter: gray; /* IE 6-9 */
  filter: url("data:image/svg+xml;utf8,<svg xmlns=\'http://www.w3.org/2000/svg\'><filter id=\'grayscale\'><feColorMatrix type=\'matrix\' values=\'0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0.3333 0.3333 0.3333 0 0 0 0 0 1 0\'/></filter></svg>#grayscale");
  -webkit-filter: grayscale(100%);
  -ms-filter: grayscale(100%);
  -o-filter: grayscale(100%);
}

body 
{
  overflow-x: hidden;
  width: 100%;
  height: 100%;
  margin: 0;
  font: 400 {{settings.font-size}}px/20px $base-font;
  color: $base-color;
  position: relative;
  background-color: $background-color;
  
  {% if settings.show-bg-image %}
    background-image: url({{'background.png' | asset_url}});
  	background-repeat: no-repeat;
  	background-position: center 0px;
    {% if settings.fix-bg-image %}
      background-attachment:fixed;
    {% endif %}
  {% endif %}
}

.serif {
  font-family: "Baskerville", "Times", serif;
}

h4.serif {
  margin-bottom: 10px;
}

.clearfix:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
 
.clearfix {
	display: inline-block;
}
 
html[xmlns] .clearfix {
	display: block;
}
 
* html .clearfix {
	height: 1%;
}

.container
{
  width: 90%;
  max-width: $max-width;
  height: auto;
  display: block;
  margin: 0 auto;
  position: relative;
 // padding: 0 0 80px 0;
}



// Styles for form elements

input[type="text"], input[type="password"], input[type="telephone"], input[type="email"], textarea, select, input[type="email"], input[type="number"]
{
  background: $form-elm-background;
  border: none;
  border-radius: 0;
  outline: none !important;
  padding: 0 14px;
  width: 100%;
  font-size: 14px;
  @include transition(all 0.1s ease);
  color: $form-elm-color;

  &:focus
  {
    background: $form-elm-focus;
  }
}

input[type="text"], input[type="email"], input[type="number"], input[type="password"], input[type="telephone"], select
{
  height: 45px;
}

label
{
  margin-bottom: 6px;
  display: block;
}

textarea
{
  padding-top: 10px;
  padding-bottom: 10px;
  height: 150px;
}

.form-group
{
  margin-bottom: 15px;

  &.or
  {
    button
    {
      margin-right: 15px;
    }

    a
    {
      margin-left: 5px;
    }
  }
}

// Icomoon settings

[class*="icon-"]
{
  font-size: 20px;
  &.fs10 { font-size: 10px; }
  &.fs15 { font-size: 15px; }
  &.fs25 { font-size: 25px; }
  &.fs30 { font-size: 30px; }

  &:hover{
    text-decoration: none !important;
  }
}

a
{
  text-decoration: none;
  @include transition(all 0.3s ease);
  color: $a-color;
  outline: none;

  &:hover
  {
    color: $a-hover-color;
    text-decoration: underline;
  }

  &.underline
  {
    text-decoration: underline;
  }
}

// Button styles

.button
{
  line-height: 45px;
  padding: 0 20px;
  font-size: 14px;
  text-align: center;
  display: inline-block;
  text-decoration: none;
  @include transition(all 0.2s ease);
  position: relative;
  border: 0;

  &:active
  {
    top: 1px;
  }

  &:hover
  {
    opacity: 0.8;
    text-decoration: none;
  }

  &.clr1
  {
    background: $btn-clr1;
    color: $btn-clr1-color;
  }

  &.clr2
  {
    background: $btn-clr2;
    color: $btn-clr2-color;
  }

  &.clr3
  {
    background: none;
    border: 2px solid $btn-clr3;
    color: $btn-clr3-color;
    padding: 0 14px;
  }
    
  &.loading
    {
      background: #DDD;
      color: white;
      cursor: default;
      opacity: 0.6;
      &:hover { opacity: 0.6; }
    }
}

.pseudo-select
{
  position: relative;

  .selected
  {
    background: $form-elm-background;
    border: none;
    border-radius: 0;
    outline: none !important;
    padding: 0 14px;
    width: 100%;
    font-size: 14px;
    @include transition(all 0.1s ease);
    color: $form-elm-color;
    line-height: 30px;
    cursor: pointer;

    &:focus
    {
      background: $form-elm-focus;
    }

    [class*="icon-"]
    {
      position: absolute;
      right: 10px;
      top: 0px;
      line-height: 30px;
      display: block;
      font-size: 15px;
      @include transition(all 0.2s ease);
    }
  }

  ul
  {
    margin: 0;
    padding: 0;
    list-style-type: none;
    //max-height: 200px;
    overflow-x: hidden;
    overflow-y: auto;
    border: 1px solid $border-color;
    // padding: 8px;
    position: absolute;
    top: 45px;
    left: 0px;
    width: 100%;
    background: #fff;
    display: none;
    z-index: 10;
    @include transition(all 0.3s ease);
    
    &.show
    {
      display: block;
    }

    li
    {
      color: #25282a;
      background: #ffffff;
      @include transition(all 0.06s ease);
      line-height: 30px;
      padding-left: 8px;
      cursor: pointer;

      &:hover
      {
        color: #ffffff;
        background: $form-elm-focus;
      }

      a
      {
        color: #25282a;
        background: #ffffff;
        @include transition(all 0.06s ease);
        display: block;

        &:hover
        {
          color: #ffffff;
          background: $a-hover-color;
        }
      }
    }
  }
}


