.notification
{
  $notification-bg: #EBEBEB;
  $notification-color: #999595;
  
  background: $notification-bg;
  @include border-radius(10px);
  padding: 15px;
  position: fixed;
  top: 30px;
  width: 90%;
  right: 5%;
  color: $notification-color;
  text-align: center;
  z-index: 9999;
  @include transition(all 0.3s ease);
  visibility: hidden;
  opacity: 0;

  &.show
  {
    visibility: visible;
    opacity: 0.95;
  }
    
  p { cursor: default; }
  
  &:hover { opacity: 0.6; }
}

