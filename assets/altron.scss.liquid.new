html {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-rendering: optimizeLegibility;
}
 
@mixin render($property, $value)
{
  -webkit-#{$property}: $value;
  -moz-#{$property}: $value;
  -o-#{$property}: $value;
  -ms-#{$property}: $value;
  #{$property}: $value;
}
 
@mixin border-box() { @include render(box-sizing, border-box); }
@mixin transition($value) { @include render(transition, $value); }
@mixin border-radius($value) { @include render(border-radius, $value); }
@mixin ul-reset() { list-style-type: none; margin: 0; padding: 0; }
input[type="text"], input[type="password"], input[type="email"], textarea, input[type="email"], input[type="number"] { -webkit-appearance: none !important; -moz-appearance: none !important; }
button { outline: none; }
.icomoon { font-family: icomoon; }

*,
*:before,
*:after 
{
  @include border-box;
}

