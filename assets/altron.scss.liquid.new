// FONTS
$heading-font: "{{settings.heading-font | split: '+' | join: " "}}";
$base-font: "{{settings.body-font | split: '+' | join: " "}}";

// COLOR SCHEME
$base-color: {{ settings.base-color }};
$border-color: {{ settings.border-color }};
$background-color: {{ settings.background-color }};
$header-color: {{ settings.header-color }};
$heading-color: {{ settings.heading-color }};

$a-color: {{ settings.a-color }};
$a-hover-color: {{ settings.a-hover-color }}; // EC6092 FFF47B

// FORM ELEMENT COLOR SCHEME
$form-elm-background: {{ settings.form-elm-background }};
$form-elm-color: {{ settings.form-elm-color }};
$form-elm-focus: {{ settings.form-elm-focus }};

// BUTTON COLOR SCHEME
$btn-clr1: {{ settings.btn-clr1 }};
$btn-clr1-color: {{ settings.btn-clr1-color }};
$btn-clr2: {{ settings.btn-clr2 }};
$btn-clr2-color: {{ settings.btn-clr2-color }};
$btn-clr3: {{ settings.btn-clr3 }};
$btn-clr3-color: {{ settings.btn-clr3-color }};

// DESKTOP NAVIGATION COLOR SCHEME
