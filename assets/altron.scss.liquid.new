// Shopping cart
#shopping-cart
{
  $cart-bg: #fafafa;

  background: $cart-bg;
  color: $base-color;

  & > div
  {
    padding: 0;

    h2
    {
      background: #fff;
      margin: 0;
      padding: 20px 25px;
      text-align: center;
      box-shadow: 0 0 3px 0 rgba(0,0,0,0.25);

      span
      {
        text-transform: none;
        display: block;
        font-size: 13px;
        margin-top: 5px;
        font-family: $base-font;
      }
    }

    .title
    {
      border-bottom: 1px solid $border-color;
      padding-bottom: 5px;
    }

    ul
    {
      margin: 0;
      padding: 0;

      li
      {
        margin: 20px 25px;
        position: relative;

        .remove
        {
          position: absolute;
          display: block;
          right: 0px;
          top: 0px;
          width: 18px;
          font-size: 12px;
          line-height: 18px;
          @include border-radius(50%);
          background: #5E5C5C;
          color: white;
          text-align: center;
          cursor: pointer;
          @include transition(all 0.3s ease);
          z-index: 2;
          opacity: 0.5;

          &:hover { opacity: 1; }
        }

        .detail
        {
          padding-left: 25px;
        }

        .info
        {
          color: #a4afbd;
          font-size: 15px;
          margin-top: 5px;
        }

        &:after
        {
          margin-left: 0;
          margin-top: 20px;
          border-bottom: 1px solid $border-color;
        }
      }
    }

    .total
    {
      border-bottom: 1px solid $border-color;
      padding: 0 0 15px 0;
      margin: 0 25px;
      margin-top: -5px;
      font-size: 16px;
      color: #a4afbd;

      &:after
      {
        clear: both;
      }

      span
      {
        float: right;
        color: #F56946;
      }
    }

    .checkout
    {
      padding: 20px 25px;
      text-align: right;
    }
  }
}

header
{
  padding: 20px 0 0 0;
  position: relative;

