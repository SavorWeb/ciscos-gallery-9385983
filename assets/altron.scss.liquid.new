$dsk-bg-color: {{ settings.dsk-bg-color }};
$dsk-a-hover-bg: {{ settings.dsk-a-hover-bg }};

$nav-a-color: {{ settings.nav-a-color }};
$nav-bg: {{ settings.nav-bg }};
$nav-border-color: {{ settings.nav-border-color }};

$footer-bg: #fbfbfa;

// MAX WIDTH OF THE CONTAINER
$max-width: 920px;


img.responsive { width: 100%; height: auto; display: block; }

