// Grid of products
.grid
{
  position: relative;
  width: 100%;
  @include ul-reset;
  margin-bottom: 50px;

  .pin
  {
    width: 200px;
    height: auto;
    position: absolute;
    visibility: hidden;
    overflow: hidden !important;
    background: #fff;

    border: 1px solid $border-color;
    border-bottom: 1px solid $border-color;

    @include transition(all 0.5s ease);

    img
    {
      width: 100%;
      height: auto;
      display: block;
      position: relative;
      @include transition(all 0.3s ease);
    }

    .product-wrap
    {
      position: relative;
      overflow: hidden;

      {% if settings.twist_scale %}

      &:hover
      {
        img
        {
          @include render(transform, rotate(-5deg) scale(1.3));
        }
      }

      {% endif %}
      
      .product-desc
      {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        visibility: hidden;
        opacity: 0;
      }
    }

    .meta
    {
      position: relative;
      font-size: 0;
      border-top: 1px solid $border-color;

      h4
      {
        padding: 0 15px 7px;
        margin: 5px 0 0;
        text-align: center;
        // width: 80%;
        width: 100%;
        float: left;
        border-bottom: 1px solid $border-color;
      }

      a, .price
      {
        line-height: 40px;
        display: inline-block;
        // width: 20%;
        color: #979393;
        text-align: center;
        @include transition(all 0.3s ease);
        vertical-align: top;
		
        float: left;
        padding: 0px 2px;
        position: relative;
        //border-left: 1px solid $border-color;
   
      }
      .icon-eye {
        width: 20%;
        border-left: 1px solid $border-color;
      }
      .price { 
        //border-right: 1px solid $border-color; 
        font-size: 15px; 
        width: 80%;

        .old-price
        {
          text-decoration: line-through;
          color: #999;
          margin-left: 10px;
          opacity: 0.8;
          display: none;
        }
      }

      a:hover
      {
        background: #f5f5f5;
        text-decoration: none;
      }
    }
  }
}

